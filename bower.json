{
  "name": "excelreport",
  "version": "1.0.0",
  "main": "path/to/main.css",
  "ignore": [
    ".jshintrc",
    "**/*.txt",
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "package.json",
    "src",
    "libs",
    "tests"
  ],
  "install": {
    "base": "src/main/webapp/libs",
    "path": {
      "js": "{name}/js",
      "css": "{name}/css",
      "/[sc|le]ss$/": "{name}/css",
      "/[png|jpg|gif|jpeg]$/": "{name}/images",
      "eot": "{name}/fonts",
      "svg": "{name}/fonts",
      "ttf": "{name}/fonts",
      "woff": "{name}/fonts"
    },
    "sources": {
      "bootstrap": [
        "bower_components/bootstrap/dist/**"
      ]
    }
  },
  "dependencies": {
    "no-bootstrap-notify": "3.1.6",
    "datatables.net": "latest",
    "datatables.net-buttons": "latest",
    "datatables.net-dt": "latest",
    "datatables.net-fixedheader": "latest",
    "datatables.net-fixedheader-dt": "latest",
    "datatables.net-buttons-dt": "latest",
    "datatables.net-select": "latest",
    "datatables.net-select-dt": "latest",
    "jszip": "latest",
    "remarkable-bootstrap-notify": "no-bootstrap-notify#3.1.6",
    "bootstrap": "4.3.1",
    "jQuery": "latest",
    "tooltipster": "4.2.6"
  },
  "devDependencies": {},
  "homepage": "https://github.com/ree566/ExcelExport",
  "authors": [
    "ree566 <ree566@yahoo.com.tw>"
  ],
  "description": "",
  "license": "MIT"
}
