<?xml version='1.0' encoding='UTF-8' ?> 
 <!--was: <?xml version="1.0" encoding="ISO-8859-1"?>--> 
<workbook>
    <worksheet name="報廢">
        
        <section startRow="0" endRow="1" />
        
        <loop startRow="1" endRow="1" items="rows" var="scrappedDetail" varType="com.advantech.model.ScrappedDetail">
            <section startRow="1" endRow="1">
                <mapping row="1" col="0">scrappedDetail.createDate</mapping>
                <mapping row="1" col="1">scrappedDetail.po</mapping>
                <mapping row="1" col="2">scrappedDetail.modelName</mapping>
                <mapping row="1" col="3">scrappedDetail.materialNumber</mapping>
                <mapping row="1" col="5">scrappedDetail.amount</mapping>
                <mapping row="1" col="6">scrappedDetail.reason</mapping>
                <mapping row="1" col="7">scrappedDetail.kind</mapping>
                <mapping row="1" col="8">scrappedDetail.price</mapping>
                <mapping row="1" col="9">scrappedDetail.negligenceUser</mapping>
                <mapping row="1" col="10">scrappedDetail.remark</mapping>
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0"></cellcheck>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
</workbook>